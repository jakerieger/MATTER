<theme
    name="Pro"
    author="Jake Rieger"
    version="1.0"
    description="Dark grey theme with good contrast"
>
    <colors>
        <color name="panel" value="2b2b2b" opacity="1.0"/>
        <color name="scene" value="323232" opacity="1.0"/>
        <color name="frame" value="1c1c1c" opacity="1.0"/>
        <color name="accent" value="545a99" opacity="1.0"/>
        <color name="border" value="1c1c1c" opacity="1.0"/>
        <color name="text" value="cccccc" opacity="1.0"/>
        <color name="text_inactive" value="8c8c8c" opacity="1.0"/>
        <color name="selected" value="404040" opacity="1.0"/>
        <color name="header" value="2b2b2b" opacity="1.0"/>
        <color name="menu" value="1c1c1c" opacity="1.0"/>
        <color name="success" value="6ce582" opacity="1.0"/>
        <color name="warning" value="de9545" opacity="1.0"/>
        <color name="error" value="e35b5b" opacity="1.0"/>
        <color name="info" value="2d89e5" opacity="1.0"/>
    </colors>
    <fonts>
        <font type="interface" name="TitilliumWeb-Regular" size="20"/>
        <font type="interface-bold" name="TitilliumWeb-Bold" size="20"/>
        <font type="console" name="FiraCode-Medium" size="16"/>
        <font type="console-bold" name="FiraCode-Bold" size="16"/>
    </fonts>
    <window
        window_rounding="0.0"
        frame_rounding="1.0"
        border_width="1.0"
    />
</theme>